{
  "name": "autoflow-server",
  "version": "1.0.0",
  "description": "AutoFlow DeFi Backend Server with Circle Programmable Wallets Integration",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:prod": "cross-env NODE_ENV=production node src/index.js",
